{
    "name": "Remini Gratis",
    "description": "Aplikasi untuk meningkatkan kualitas foto menggunakan API Pexels",
    "keywords": ["photo", "enhancer", "Pexels"],
    "website": "https://xnxx.com",
    "repository": "https://github.com/Grizly9/web",
    "logo": "https://xnxx.com",
    "success_url": "/welcome",
    "scripts": {
        "postdeploy": "bundle exec rake bootstrap"
    },
    "formation": {
        "web": {
            "quantity": 1,
            "size": "eco"
        }
    },
    "image": "heroku/ruby",
    "addons": [
        "openredis",
        {
            "plan": "mongolab:shared-single-small",
            "as": "MONGO"
        },
        {
            "plan": "heroku-postgresql",
            "options": {
                "version": "9.5"
            }
        }
    ],
    "buildpacks": [
        {
            "url": "https://github.com/stomita/heroku-buildpack-phantomjs"
        }
    ]
}
